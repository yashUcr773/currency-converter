# Production Environment Variables
# Copy this file to .env.production.local and fill in your production values

# Backend API Configuration
VITE_BACKEND_URL=https://your-production-backend.vercel.app

# Clerk Authentication (required for cloud sync)
VITE_CLERK_PUBLISHABLE_KEY=pk_live_your_production_clerk_key_here

# Analytics (optional)
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Feature Flags
VITE_ENABLE_CLOUD_SYNC=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# API Rate Limiting
VITE_API_RATE_LIMIT=100

# Cache TTL (in milliseconds)
VITE_CACHE_TTL=86400000
